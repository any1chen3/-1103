<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- CityMapper.xml -->
<mapper namespace="com.military.asset.mapper.CityMapper">
    <select id="selectByName" resultType="com.military.asset.entity.City">
        SELECT * FROM city WHERE name LIKE CONCAT('%', #{name}, '%')
    </select>

    <select id="selectAll" resultType="com.military.asset.entity.City">
        SELECT code, name, parent_code AS parentCode, level FROM city
    </select>
</mapper>